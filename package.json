{
  "name": "demo-nodejs-unit-test",
  "version": "1.0.0",
  "description": "demostracion pruebas unitarias con Jest",
  "main": "index.js",
  "scripts": { 
    "test": "jest --runInBand --config jest.config.json",
    "test:watch": "npm test -- --watchAll --verbose --runInBand --notify",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch"
  },
